<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="8.0dip"
    android:layout_marginBottom="8.0dip"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="16.0dip"
        android:layout_marginEnd="16.0dip">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1.0">
            <com.google.android.material.card.MaterialCardView
                android:layout_width="wrap_content"
                android:layout_height="200.0dip"
                android:layout_weight="1.0"
                android:onClick="uwuRepository"
                android:layout_marginEnd="8.0dip"
                app:strokeWidth="0.0dip"
                app:cardBackgroundColor="#00000000"
                app:cardCornerRadius="24.0dip"
                app:cardElevation="0.0dip">
                <FrameLayout
                    android:background="@drawable/uwu_bg_sin"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <LinearLayout
                            android:background="@drawable/uwu_google_shape"
                            android:layout_width="45.0dip"
                            android:layout_height="45.0dip"
                            android:layout_marginTop="16.0dip"
                            android:layout_marginStart="16.0dip"
                            android:backgroundTint="?attr/colorPrimary">
                            <ImageView
                                android:gravity="center"
                                android:layout_width="25.0dip"
                                android:layout_height="25.0dip"
                                android:layout_marginTop="10.0dip"
                                android:src="@drawable/uwu_github"
                                android:layout_marginStart="10.0dip"
                                app:tint="?attr/colorIcon" />
                        </LinearLayout>
                        <RelativeLayout
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:paddingTop="10.0dip"
                            android:paddingBottom="10.0dip"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1.0"
                            android:layout_marginStart="20.0dip"
                            android:layout_marginEnd="20.0dip">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
                                <TextView
                                    android:textSize="17.0sp"
                                    android:textColor="?android:textColorPrimary"
                                    android:ellipsize="end"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="GitHub"
                                    android:singleLine="true" />
                                <TextView
                                    android:textSize="14.0sp"
                                    android:textColor="?android:textColorSecondary"
                                    android:ellipsize="end"
                                    android:paddingTop="0.0dip"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Source code repository"
                                    android:singleLine="false" />
                            </LinearLayout>
                        </RelativeLayout>
                        <LinearLayout
                            android:gravity="end"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0.0dip"
                            android:layout_marginBottom="16.0dip"
                            android:layout_marginEnd="-6.0dip">
                            <LinearLayout
                                android:background="@drawable/uwu_bg_circle_arrow"
                                android:layout_width="28.0dip"
                                android:layout_height="28.0dip"
                                android:layout_marginEnd="25.0dip"
                                android:backgroundTint="?attr/colorBg">
                                <ImageView
                                    android:layout_width="25.0dip"
                                    android:layout_height="25.0dip"
                                    android:layout_marginTop="1.0dip"
                                    android:src="@drawable/uwu_arrow"
                                    app:tint="?android:textColorPrimary"
                                    android:layout_marginStart="2.0dip" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1.0">
            <com.google.android.material.card.MaterialCardView
                android:layout_width="wrap_content"
                android:layout_height="200.0dip"
                android:layout_weight="1.0"
                android:onClick="uwuCredits"
                android:layout_marginStart="8.0dip"
                app:strokeWidth="0.0dip"
                app:cardBackgroundColor="#00000000"
                app:cardCornerRadius="24.0dip"
                app:cardElevation="0.0dip">
                <FrameLayout
                    android:background="@drawable/uwu_bg_sin"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <LinearLayout
                            android:background="@drawable/uwu_google_shape"
                            android:layout_width="45.0dip"
                            android:layout_height="45.0dip"
                            android:layout_marginTop="16.0dip"
                            android:layout_marginStart="16.0dip"
                            android:backgroundTint="?attr/colorPrimary">
                            <ImageView
                                android:gravity="center"
                                android:layout_width="25.0dip"
                                android:layout_height="25.0dip"
                                android:layout_marginTop="10.0dip"
                                android:src="@drawable/uwu_credits"
                                android:layout_marginStart="10.0dip"
                                app:tint="?attr/colorIcon" />
                        </LinearLayout>
                        <RelativeLayout
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:paddingTop="10.0dip"
                            android:paddingBottom="10.0dip"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1.0"
                            android:layout_marginStart="20.0dip"
                            android:layout_marginEnd="20.0dip">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">
